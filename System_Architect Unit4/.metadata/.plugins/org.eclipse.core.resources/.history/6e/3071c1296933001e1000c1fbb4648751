#ifndef LIFO_H_
#define LIFO_H_

/*****************************************_Includes_*******************************************/
#include<stdint.h>


/**************************************_DataType_Config***************************************/
									/*choose the data type*/
#define DataType		uint32_t

//Type Definitions
typedef struct LIFO_buffer {
	DataType  length;
	DataType  count;
	DataType* base;
	DataType* head;

};


#endif /* LIFO_H_ */
