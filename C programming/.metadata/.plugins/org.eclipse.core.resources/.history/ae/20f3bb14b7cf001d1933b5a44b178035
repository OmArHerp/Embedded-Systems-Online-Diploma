#include<stdio.h>
#include<string.h>

void main()
{
	char string1[100]; char c; int i=0; int count=0;
	printf("please enter the string first:\n");
	scanf("%s",string1);
	printf("\nNow enter the character you want to know it's frequency: ");
	scanf("%c",&c);

	while(string1[i]!=0)
	{
		if(strlwr(string1[i]) == strlwr(c))
			count++;
		i++;
	}
}

