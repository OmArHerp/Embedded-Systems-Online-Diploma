#include "FIFO.h"

FIFO_status FIFO_Init(FIFO_Buf_t* queue , DataType* buf, uint32_t length)
{
	if (buf == NULL)
		return FIFO_Null ;
	queue->base = buf;
	queue->head = buf;
	queue->tail = buf;
	queue->length = length;
	queue->count = 0;

	return FIFO_No_Error ;
}
